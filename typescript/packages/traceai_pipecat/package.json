{
  "name": "@traceai/fi-instrumentation-pipecat",
  "version": "0.1.0",
  "description": "OpenTelemetry instrumentation for Pipecat voice AI pipeline SDK",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "build:watch": "tsc -w",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "opentelemetry",
    "tracing",
    "instrumentation",
    "pipecat",
    "voice",
    "ai",
    "pipeline",
    "real-time",
    "observability"
  ],
  "author": "FutureAGI",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/futureagi/traceai.git",
    "directory": "typescript/packages/traceai_pipecat"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/instrumentation": "^0.46.0",
    "@traceai/fi-core": "workspace:*",
    "@traceai/fi-semantic-conventions": "workspace:*"
  },
  "devDependencies": {
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@pipecat-ai/client-js": ">=0.2.0"
  },
  "peerDependenciesMeta": {
    "@pipecat-ai/client-js": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
