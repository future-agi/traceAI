{
  "name": "@traceai/pgvector",
  "version": "0.1.0",
  "description": "TraceAI instrumentation for pgvector (PostgreSQL vector extension)",
  "main": "./dist/src/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --build tsconfig.json tsconfig.esm.json && tsc-alias -p tsconfig.esm.json && pnpm run postbuild",
    "postbuild": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json",
    "test": "jest",
    "lint": "eslint . --ext .ts"
  },
  "keywords": ["traceai", "opentelemetry", "instrumentation", "pgvector", "postgresql", "vector-database"],
  "author": "Future AGI <no-reply@futureagi.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/instrumentation": "^0.46.0",
    "@traceai/fi-core": "workspace:*",
    "@traceai/fi-semantic-conventions": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "pg": "^8.11.0",
    "pgvector": "^0.1.8"
  },
  "peerDependencies": {
    "pg": ">=8.0.0",
    "pgvector": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "pg": { "optional": true },
    "pgvector": { "optional": true }
  },
  "files": ["dist", "README.md"]
}
