{
  "name": "@traceai/guardrails",
  "version": "0.1.0",
  "description": "OpenTelemetry instrumentation for Guardrails AI",
  "main": "./dist/src/index.js",
  "module": "./dist/esm/index.js",
  "esnext": "./dist/esnext/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --build tsconfig.json tsconfig.esm.json tsconfig.esnext.json && tsc-alias -p tsconfig.esm.json && pnpm run postbuild",
    "postbuild": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json",
    "test": "jest",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "opentelemetry",
    "tracing",
    "instrumentation",
    "guardrails",
    "validation",
    "llm",
    "observability"
  ],
  "author": "Future AGI <no-reply@futureagi.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.202.0",
    "@traceai/fi-core": "^0.1.13",
    "@traceai/fi-semantic-conventions": "^0.1.9"
  },
  "devDependencies": {
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/sdk-trace-node": "^1.30.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@guardrails-ai/core": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@guardrails-ai/core": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "src",
    "!**/__tests__/**"
  ],
  "publishConfig": {
    "access": "public"
  }
}
