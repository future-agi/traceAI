{
  "name": "fi-instrumentation-otel-ts",
  "version": "0.1.0",
  "private": true,
  "description": "OpenTelemetry instrumentation for TypeScript",
  "scripts": {
    "build": "pnpm run build:ordered",
    "build:deps": "pnpm --filter @traceai/fi-core --filter @traceai/fi-semantic-conventions run build",
    "build:consumers": "pnpm --filter @traceai/openai --filter @traceai/anthropic --filter @traceai/langchain --filter @traceai/bedrock --filter @traceai/vercel --filter @traceai/chromadb --filter @traceai/pinecone --filter @traceai/qdrant --filter @traceai/weaviate --filter @traceai/milvus --filter @traceai/lancedb --filter @traceai/mongodb --filter @traceai/pgvector --filter @traceai/redis --filter @traceai/together --filter @traceai/huggingface --filter @traceai/vllm --filter @traceai/deepseek --filter @traceai/fireworks --filter @traceai/cerebras --filter @traceai/xai run build",
    "build:ordered": "pnpm run build:deps && pnpm run build:consumers",
    "test": "pnpm run -r test",
    "test:coverage": "pnpm run -r test:coverage",
    "test:watch": "pnpm run -r test:watch",
    "prepublishOnly": "pnpm run build && pnpm run test && pnpm run lint",
    "lint": "eslint . --ext .ts",
    "test:e2e": "pnpm --filter traceai-e2e-tests run test",
    "test:e2e:chromadb": "pnpm --filter traceai-e2e-tests run test:chromadb",
    "test:e2e:qdrant": "pnpm --filter traceai-e2e-tests run test:qdrant",
    "e2e:docker:up": "pnpm --filter traceai-e2e-tests run docker:up",
    "e2e:docker:down": "pnpm --filter traceai-e2e-tests run docker:down"
  },
  "keywords": [
    "opentelemetry",
    "instrumentation",
    "typescript"
  ],
  "author": "Future AGI <no-reply@futureagi.com>",
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.15.29",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@traceai/fi-semantic-conventions": "workspace:*",
    "@traceai/fi-core": "workspace:*",
    "@traceai/openai": "workspace:*",
    "@traceai/anthropic": "workspace:*",
    "@traceai/langchain": "workspace:*",
    "@traceai/bedrock": "workspace:*",
    "@traceai/vercel": "workspace:*",
    "@traceai/chromadb": "workspace:*",
    "@traceai/pinecone": "workspace:*",
    "@traceai/qdrant": "workspace:*",
    "@traceai/weaviate": "workspace:*",
    "@traceai/milvus": "workspace:*",
    "@traceai/lancedb": "workspace:*",
    "@traceai/mongodb": "workspace:*",
    "@traceai/pgvector": "workspace:*",
    "@traceai/redis": "workspace:*",
    "@traceai/together": "workspace:*",
    "@traceai/huggingface": "workspace:*",
    "@traceai/vllm": "workspace:*",
    "@traceai/deepseek": "workspace:*",
    "@traceai/fireworks": "workspace:*",
    "@traceai/cerebras": "workspace:*",
    "@traceai/xai": "workspace:*",
    "uuid": "^11.1.0"
  },
  "packageManager": "pnpm@10.30.1+sha512.3590e550d5384caa39bd5c7c739f72270234b2f6059e13018f975c313b1eb9fefcc09714048765d4d9efe961382c312e624572c0420762bdc5d5940cdf9be73a"
}
